    println (''' Generated by HelpNDoc - https://www.helpndoc.com');
    println ('');
    FOR nCurTopic := 0 TO length(aTopicList) - 1 DO
    begin
        printfln('%%HELP_%s = %d', [aTopicList[nCurTopic].HelpId, aTopicList[nCurTopic].HelpContext]);
    end;

end.
